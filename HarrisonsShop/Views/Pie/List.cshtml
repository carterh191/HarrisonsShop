@*@model IEnumerable<HarrisonsShop.Models.Pie> //Razor syntax, access properties on model that will be passed(the instance)*@
@model HarrisonsShop.ViewModels.PieListViewModel



<h1>@Model.CurrentCategory</h1>
@foreach(var pie in Model.Pies)
{
 <div>
            <h2>@pie.Name</h2>
            <h2>@pie.Price.ToString("c")</h2>
            <h4>@pie.Category.CategoryName</h4>
        </div>
    }
